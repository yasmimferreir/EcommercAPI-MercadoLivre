{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["unix", "window"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "jsx-quotes": [2, "prefer-double"],
    "eol-last": ["error", "always"]
  }
}
